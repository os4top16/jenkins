#!groovy
String buildShell = "${env.buildShell}"
pipeline{
  agent{ node{ label 'master' }}
  stages{
    stage("maven build"){
      steps{
        script{
          mvHome = tool 'M2'
          sh '${mvHOME}/bin/mvn ${buildShell}'
        }
      }
    }
    stage("ant build"){
      steps{
        script{
          antHome = tool 'ANT'
          sh '${antHome}/bin/ant ${buildShell}'
        }
      }
    }
  }

}
